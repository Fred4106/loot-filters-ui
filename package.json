{
    "name": "loot-filters-ui",
    "version": "1.0.0",
    "homepage": "https://kaqemeex.github.io/loot-filters-ui/",
    "private": true,
    "scripts": {
        "start": "webpack serve --mode development --devtool source-map",
        "build": "webpack --mode production --devtool source-map",
